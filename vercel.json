{
  "version": 2,
  "builds": [
    {
      "src": "Backend/app.py",
      "use": "@vercel/python",
      "config": {
        "runtime": "python3.10"
      }
    },
    {
      "src": "React-Frontend/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "Backend/app.py"
    },
    {
      "src": "/(.*)",
      "dest": "React-Frontend/$1"
    }
  ]
}
